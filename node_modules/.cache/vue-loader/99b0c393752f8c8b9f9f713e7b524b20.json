iconName.includes(\"el-icon\");\n    });\n\n    return {\n      ...toRefs(_this),\n      isElementUiIcon,\n    };\n  },\n});\n</script>\n\n<style lang=\"scss\" scoped>\ni {\n  font-size: calc(1vw * #{$heightRatio} * v-bind(fontSize));\n  color: v-bind(color);\n}\n\nspan {\n  // font-size: 16px;\n  font-size: calc(1vw * #{$heightRatio} * v-bind(fontSize));\n  color: v-bind(color);\n  transition: color 0.3s;\n  display: flex;\n  align-items: center;\n}\n\n.svg-icon {\n  width: 1em;\n  height: 1em;\n  vertical-align: -0.15em;\n  fill: currentColor;\n  overflow: hidden;\n  cursor: pointer;\n}\n</style>\n"]}]}