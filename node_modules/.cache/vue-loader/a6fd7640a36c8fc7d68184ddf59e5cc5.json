child(5) {\n          @include Width(60);\n        }\n      }\n    }\n    .list {\n      width: 100%;\n      @include wHeight(220);\n      overflow-y: scroll;\n      // display: flex;\n      // justify-content: space-between;\n      // flex-direction: column;\n      @include MarginTop(10);\n      @include PaddingLeft(6);\n      box-sizing: border-box;\n      overflow-y: scroll;\n      .item {\n        width: 100%;\n        @include wHeight(40);\n        @include LineHeight(40);\n        @include MarginBottom(5);\n        display: flex;\n        justify-content: space-between;\n        // background-color: rgba(255, 255, 255, 0.1);\n        span {\n          width: 100%;\n          display: inline-block;\n          @include wHeight(40);\n          @include FontSize(12);\n          @include LineHeight(40);\n          color: rgba(255, 255, 255, 0.78);\n          text-align: center;\n          &:nth-child(1) {\n            @include Width(120);\n          }\n          &:nth-child(2) {\n            @include Width(90);\n          }\n          &:nth-child(3) {\n            @include Width(120);\n          }\n          &:nth-child(4) {\n            @include Width(90);\n          }\n          &:nth-child(5) {\n            @include Width(60);\n            cursor: pointer;\n            i {\n              display: inline-block;\n              @include Width(45);\n              @include wHeight(20);\n              @include LineHeight(20);\n              text-align: center;\n              background-color: #0f74a3;\n              @include BorderRadius(4);\n              @include PaddingLeft(8);\n              color: rgba(255, 255, 255, 0.8);\n              position: relative;\n              &::before {\n                content: \"\";\n                position: absolute;\n                left: 0;\n                @include Top(2);\n                @include Width(14);\n                @include wHeight(14);\n                background: url(\"~@/assets/images/wisdom/下一步.svg\") no-repeat