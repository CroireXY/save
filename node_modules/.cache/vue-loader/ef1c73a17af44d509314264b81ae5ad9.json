\n        <span>\r\n          {{ \"无警报电磁\" }}\r\n        </span>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"logo\">\r\n      <div class=\"tit\" @click=\"reset\">飞服平台</div>\r\n    </div>\r\n\r\n    <div class=\"info-right\">\r\n      <div class=\"tool\">\r\n        <div @click=\"eleMapClick\">\r\n          <el-tooltip\r\n            popper-class=\"tip-popper\"\r\n            class=\"box-item\"\r\n            effect=\"dark\"\r\n            content=\"加载vtpk\"\r\n            placement=\"bottom\"\r\n          >\r\n            <Icon\r\n              :color=\"eleMap ? '#7afafe' : '#fff'\"\r\n              :font-size=\"25\"\r\n              icon=\"tool-bar-ememap\"\r\n            />\r\n          </el-tooltip>\r\n        </div>\r\n        <div @click=\"TechnologyClick\">\r\n          <el-tooltip\r\n            popper-class=\"tip-popper\"\r\n            class=\"box-item\"\r\n            effect=\"dark\"\r\n            content=\"科技\"\r\n            placement=\"bottom\"\r\n          >\r\n            <Icon\r\n              :color=\"Technologyshow ? '#7afafe' : '#fff'\"\r\n              :font-size=\"25\"\r\n              icon=\"tool-bar-keji\"\r\n            />\r\n          </el-tooltip>\r\n        </div>\r\n        <div @click=\"UIClick\">\r\n          <el-tooltip\r\n            class=\"box-item\"\r\n            effect=\"dark\"\r\n            content=\"UI显隐\"\r\n            placement=\"bottom\"\r\n          >\r\n            <Icon\r\n              :color=\"!UIShow ? '#7afafe' : '#fff'\"\r\n              :font-size=\"26\"\r\n              icon=\"mianban\"\r\n            />\r\n          </el-tooltip>\r\n        </div>\r\n        <div @click=\"LayerTreeClick\">\r\n          <el-tooltip\r\n            popper-class=\"tip-popper\"\r\n            class=\"box-item\"\r\n            effect=\"dark\"\r\n            content=\"图层树\"\r\n            placement=\"bottom\"\r\n          >\r\n            <Icon\r\n              :color=\"LayerTreeshow ? '#7afafe' : '#fff'\"\r\n              :font-size=\"30\"\r\n              icon=\"tucengshu\"\r\n            />\r\n          </el-tooltip>\r\n        </div>\r\n        <div @click=\"AnimationClick\">\r\n          <el-tooltip\r\n            popper-class=\"tip-popper\"\r\n            class=\"box-item\"\r\n            effect=\"dark\"\r\n            content=\"导览\"\r\n            placement=\"bottom\"\r\n          >\r\n            <Icon\r\n              :color=\"AnimationShow ? '#7afafe' : '#fff'\"\r\n              :font-size=\"26\"\r\n              icon=\"xunimanyou\"\r\n            />\r\n          </el-tooltip>\r\n        </div>\r\n\r\n        <div @click=\"WeatherClick\">\r\n          <el-tooltip\r\n            popper-class=\"tip-popper\"\r\n            class=\"box-item\"\r\n            effect=\"dark\"\r\n            content=\"气象\"\r\n            placement=\"bottom\"\r\n          >\r\n            <Icon\r\n              :color=\"WeatherShow ? '#7afafe' : '#fff'\"\r\n              :font-size=\"30\"\r\n              icon=\"qixiangjiance\"\r\n            />\r\n          </el-tooltip>\r\n        </div>\r\n        <!-- 工具栏 -->\r\n        <div @click=\"toolBarShowClick\" class=\"too-bar-style\">\r\n          <el-tooltip\r\n            popper-class=\"tip-popper\"\r\n            class=\"box-item\"\r\n            effect=\"dark\"\r\n            content=\"工具栏\"\r\n            placement=\"bottom\"\r\n          >\r\n            <Icon\r\n              :color=\"ToolBarShow ? '#7afafe' : '#fff'\"\r\n              :font-size=\"24\"\r\n              icon=\"tool-bar\"\r\n            />\r\n          </el-tooltip>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n  <div class=\"left-bg\" v-show=\"UIShow\">\r\n    <div class=\"weather-box\">\r\n      <div class=\"alarm-item\">\r\n        <div class=\"label\">温度</div>\r\n         <div class=\"value\">23°C</div>\r\n      </div>\r\n\r\n      <div class=\"weather-item\">\r\n        <div class=\"label\">能见度</div>\r\n        <div class=\"value\">3km</div>\r\n      </div>\r\n